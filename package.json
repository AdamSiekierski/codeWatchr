{
  "name": "codewatchr",
  "productName": "codeWatchr",
  "version": "0.1.0",
  "description": "An app that maintains your coding sessions. Time, lines of code, etc. Still in development",
  "main": "src/electron.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src"
  },
  "keywords": [],
  "author": "Adam Siekierski",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "codewatchr"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "AdamSiekierski",
        "name": "codeWatchr"
      },
      "windowsStoreConfig": {
        "packageName": "codewatchr",
        "name": "codewatchr"
      }
    }
  },
  "dependencies": {
    "@hot-loader/react-dom": "16.8.6",
    "chokidar": "^3.0.0",
    "electron": "^4.0.0",
    "electron-compile": "^6.4.4",
    "electron-compilers": "^5.9.0",
    "electron-devtools-installer": "^2.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-util": "^0.11.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-router-dom": "^5.0.0",
    "react-svg": "^9.0.4",
    "semantic-ui-react": "^0.87.3",
    "statman": "^1.7.0",
    "styled-components": "^4.2.0",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.6.0",
    "text-file-follower": "^0.1.0",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7"
  }
}
